{
  "name": "vue3-realworld-example-app",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "build": "nodemon build.js --watch src -e ts,vue",
    "serve": "nodemon dist/server/entry-server --watch dist",
    "build-production": "node build.js",
    "serve-production": "node dist/server/entry-server",
    "check-ts-errors": "yarn tsc --noEmit && yarn vti diagnostics",
    "lint": "eslint \"src/**/*.{js,ts,vue}\"",
    "test": "yarn check-ts-errors && yarn lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@harlem/core": "^1.1.1",
    "@harlem/plugin-ssr": "^1.1.1",
    "@vue/server-renderer": "^3.0.7",
    "insane": "^2.6.2",
    "js-cookie": "^2.2.1",
    "koa": "^2.13.1",
    "koa-send": "^5.0.1",
    "marked": "^2.0.1",
    "node-fetch": "^2.6.1",
    "vue": "^3.0.7",
    "vue-router": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@types/js-cookie": "^2.2.6",
    "@types/koa-send": "^4.1.2",
    "@types/marked": "^2.0.0",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.16.1",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.7",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "^5.1.2",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.6",
    "rollup-plugin-analyzer": "^4.0.0",
    "typescript": "~4.2.2",
    "vite": "^2.0.5",
    "vti": "^0.1.1"
  }
}
